  <Config Name="REDIS_HOST" Target="REDIS_HOST" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">localhost</Config>
  <Config Name="CELERY_BROKER_URL" Target="CELERY_BROKER_URL" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">redis://localhost:6379/0</Config>
